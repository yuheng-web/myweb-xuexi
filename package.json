    "start": "cross-env PORT=3006 react-scripts start",
    "start.win": "set PORT=5000 && react-scripts start",
    "build": "rm -rf $(pwd)/build && react-scripts build",
    "build.api": "cp -r public/api build/api",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
